      // è®¾ç½®yè½´çº¿çš„é¢œè‰²
      yAxis.setAxisLineColor(chart.getResources().getColor(R.color.color_999999));
      // è®¾ç½®yè½´çš„æ–‡å­—é¢œè‰²
      yAxis.setTextColor(chart.getResources().getColor(R.color.color_ffab01));
      // è®¾ç½®yè½´æ–‡å­—çš„å¤§å°
      yAxis.setTextSize(12);
      // è®¾ç½®yè½´æ•°æ®åç§»é‡ (labelsè·ç¦»xxè½´çš„åç§»é‡)
      //è®¾ç½®yè½´labelsæ¨ªå‘ç§»åŠ¨è¾¹è·,è´Ÿæ•°å¾€å·¦ç§»,æ­£æ•°å¾€å³ç§»
      yAxis.setXOffset(10);

      BarDataSet   set1 = new BarDataSet(entries, title);
      set1.setColor(barColor);
      ArrayList<IBarDataSet> dataSets = new ArrayList<IBarDataSet>();
      dataSets.add(set1);
      BarData data = new BarData(dataSets);
      // è®¾ç½®valueå€¼ é¢œè‰²
      data.setValueTextColor(chart.getResources().getColor(R.color.color_c4c3ce));
      //è®¾ç½®yè½´æ˜¾ç¤ºçš„æ ‡ç­¾
      data.setValueFormatter(new ValueFormatter() {
          @Override
          public String getFormattedValue(float value, Entry entry, int dataSetIndex, ViewPortHandler viewPortHandler) {
              return ((int) (value * 100)) + "%";
          }
      });
      //æ˜¯å¦æ˜¾ç¤ºæŸ±çŠ¶å›¾é¡¶éƒ¨å€¼
      set1.setDrawValues(false);
      // è®¾ç½®barçš„å®½åº¦
      data.setBarWidth(0.35f);
      //è®¾ç½®æ•°æ®
      chart.setData(data);

      LineDataSet dataSet = new LineDataSet(entries, label);
      // è®¾ç½®æ›²çº¿çš„é¢œè‰²
      dataSet.setColor(lineColor);
      // è®¾ç½®çº¿çš„å®½åº¦
      dataSet.setLineWidth(2f);
      //è®¾ç½®åœ†ç‚¹çš„é¢œè‰²
      dataSet.setCircleColor(Color.WHITE);
      // è®¾ç½®å¤–åœ†ç‚¹åŠå¾„
      dataSet.setCircleRadius(5f);
      //è®¾ç½®å†…åœ†çš„é¢œè‰²
      dataSet.setCircleHoleColor(lineColor);
      //è®¾ç½®å†…åœ†åŠå¾„
      dataSet.setCircleHoleRadius(3f);
      // è®¾ç½®å›¾æ ‡æ¨¡å¼ä¸ºè´å¡å°”æ›²çº¿,æœ‰å…¶ä»–æ¨¡å¼å¯ä»¥æ ¹æ®éœ€è¦è‡ªè¡Œé€‰æ‹©
      dataSet.setMode(LineDataSet.Mode.CUBIC_BEZIER);
      //è®¾ç½®æŒ‡å®šæ­¤æ•°æ®é›†åº”è¯¥é’ˆå¯¹å“ªä¸ªè½´ç»˜åˆ¶(å·¦è¾¹æˆ–è€…å³è¾¹)
      dataSet.setAxisDependency(YAxis.AxisDependency.LEFT);
      // æ˜¯å¦ç»˜åˆ¶æ•°æ®å€¼
      dataSet.setDrawValues(false);


