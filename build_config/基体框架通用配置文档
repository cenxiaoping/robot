      xAxis.setYOffset(5f);
      xAxis.setXOffset(200f);
      // æ˜¾ç¤ºxè½´æ ‡ç­¾
      ValueFormatter formatter = new ValueFormatter() {
          @Override
          public String getFormattedValue(float value) {
              int index = (int) value;
              if (index < 0 || index >= labels.size()) {
                  return "";
              }
              return labels.get(index);
              //return labels.get(Math.min(Math.max((int) value, 0), labels.size() - 1));
          }
      };
      // å¼•ç”¨æ ‡ç­¾
      xAxis.setValueFormatter(formatter);
      // è®¾ç½®xè½´æ–‡å­—é¢œè‰²
      xAxis.setTextColor(chart.getResources().getColor(R.color.color_444444));
      float axisMinimum = -0.5f;
      float axisMaximum = 8.5f;
      // ä¸ºæ­¤è½´è®¾ç½®è‡ªå®šä¹‰æœ€å°å€¼ã€‚å¦‚æœè®¾ç½®ï¼Œåˆ™ä¸è®¡ç®—æ­¤å€¼,è‡ªåŠ¨å–å†³äº
         æä¾›çš„æ•°æ®ã€‚ä½¿ç”¨resetAxisMin Valueï¼ˆï¼‰æ¥æ’¤æ¶ˆè¿™ä¸ª.
         å¦‚æœè®¾ç½®StartAtZeroï¼ˆfalseï¼‰
         è¿™ä¸ªæ–¹æ³•ã€‚å¦åˆ™ï¼Œè½´-æœ€å°å€¼ä»å°†è¢«å¼ºåˆ¶ä¸º0ã€‚
      xAxis.setAxisMinimum(axisMinimum);//è®¾ç½®ç¦»å·¦è¾¹ä½ç½®0.5ä¸ªæŸ±çŠ¶å›¾çš„å®½åº¦
     // è®¾ç½®æ€»çš„å®½åº¦ï¼Œå½“å‰å·¦å³å„ç©º1.5ä¸ªæŸ±çŠ¶å›¾çš„å®½åº¦ ,è¿™æ ·ä¸¤è¾¹å„ç©ºå‡ºæ¥1.5ä¸ªæŸ±çŠ¶å›¾çš„è·ç¦»ã€‚
      xAxis.setAxisMaximum(axisMaximum);
      // è®¾ç½®xè½´æ¯æœ€å°åˆ»åº¦
      xAxis.setGranularity(1f);
      //è®¾ç½®æ ‡ç­¾å±…ä¸­
      xAxis.setCenterAxisLabels(false);
      //å›¾è¡¨å°†é¿å…ç¬¬ä¸€ä¸ªå’Œæœ€åä¸€ä¸ªæ ‡ç­¾æ¡ç›®è¢«å‡æ‰åœ¨å›¾è¡¨æˆ–å±å¹•çš„è¾¹ç¼˜
      xAxis.setAvoidFirstLastClipping(false);
      // è®¾ç½®yè½´label æ•°é‡
      xAxis.setLabelCount(labels.size(), false);

      //yè½´å…ˆå…³è®¾ç½®
      YAxis yAxis = chart.getAxisLeft();
      //è®¾ç½®yè½´labels çš„æ•°å€¼ä¿ç•™ä¸€ä½å°æ•°ç‚¹æ˜¾ç¤º,å¦å¤–ä¸€ç§å°±æ˜¯æ˜¾ç¤ºæ•´å‹æ•°æ®
      yAxis.chatType = yAxis.DOUBLE;
      //è®¾ç½®xè½´çš„æœ€å¤§å€¼
      yAxis.setAxisMaximum(150.0f + 7.5f);
      // è®¾ç½®yè½´çš„æœ€å¤§å€¼
      yAxis.setAxisMinimum(0.0f);
      // è®¾ç½®yè½´çš„æœ€å°åˆ»åº¦
      yAxis.setGranularity(15);
      // è®¾ç½®yè½´label æ•°é‡
      yAxis.setLabelCount(11, false);
      // æ˜¾ç¤ºyè½´
      yAxis.setDrawAxisLine(true);
      // è®¾ç½®yè½´æ•°æ®çš„ä½ç½®
      yAxis.setPosition(YAxis.YAxisLabelPosition.OUTSIDE_CHART);
      // ä¸ä»yè½´å‘å‡ºæ¨ªå‘ç›´çº¿
      yAxis.setDrawGridLines(false);
      // æ˜¯å¦æ˜¾ç¤ºyè½´åæ ‡çº¿
      yAxis.setDrawZeroLine(true);
      // è®¾ç½®yè½´çº¿çš„é¢œè‰²
      yAxis.setAxisLineColor(chart.getResources().getColor(R.color.color_999999));
      // è®¾ç½®yè½´çš„æ–‡å­—é¢œè‰²
      yAxis.setTextColor(chart.getResources().getColor(R.color.color_ffab01));
      // è®¾ç½®yè½´æ–‡å­—çš„å¤§å°
      yAxis.setTextSize(12);
      // è®¾ç½®yè½´æ•°æ®åç§»é‡ (labelsè·ç¦»xxè½´çš„åç§»é‡)
      //è®¾ç½®yè½´labelsæ¨ªå‘ç§»åŠ¨è¾¹è·,è´Ÿæ•°å¾€å·¦ç§»,æ­£æ•°å¾€å³ç§»
      yAxis.setXOffset(10);

      BarDataSet   set1 = new BarDataSet(entries, title);
      set1.setColor(barColor);
      ArrayList<IBarDataSet> dataSets = new ArrayList<IBarDataSet>();
      dataSets.add(set1);
      BarData data = new BarData(dataSets);
      // è®¾ç½®valueå€¼ é¢œè‰²
      data.setValueTextColor(chart.getResources().getColor(R.color.color_c4c3ce));
      //è®¾ç½®yè½´æ˜¾ç¤ºçš„æ ‡ç­¾
      data.setValueFormatter(new ValueFormatter() {
          @Override
          public String getFormattedValue(float value, Entry entry, int dataSetIndex, ViewPortHandler viewPortHandler) {
              return ((int) (value * 100)) + "%";
          }
      });
      //æ˜¯å¦æ˜¾ç¤ºæŸ±çŠ¶å›¾é¡¶éƒ¨å€¼
      set1.setDrawValues(false);
      // è®¾ç½®barçš„å®½åº¦
      data.setBarWidth(0.35f);
      //è®¾ç½®æ•°æ®
      chart.setData(data);

      LineDataSet dataSet = new LineDataSet(entries, label);
      // è®¾ç½®æ›²çº¿çš„é¢œè‰²
      dataSet.setColor(lineColor);
      // è®¾ç½®çº¿çš„å®½åº¦
      dataSet.setLineWidth(2f);
      //è®¾ç½®åœ†ç‚¹çš„é¢œè‰²
      dataSet.setCircleColor(Color.WHITE);
      // è®¾ç½®å¤–åœ†ç‚¹åŠå¾„
      dataSet.setCircleRadius(5f);
      //è®¾ç½®å†…åœ†çš„é¢œè‰²
      dataSet.setCircleHoleColor(lineColor);
      //è®¾ç½®å†…åœ†åŠå¾„
      dataSet.setCircleHoleRadius(3f);
      // è®¾ç½®å›¾æ ‡æ¨¡å¼ä¸ºè´å¡å°”æ›²çº¿,æœ‰å…¶ä»–æ¨¡å¼å¯ä»¥æ ¹æ®éœ€è¦è‡ªè¡Œé€‰æ‹©
      dataSet.setMode(LineDataSet.Mode.CUBIC_BEZIER);
      //è®¾ç½®æŒ‡å®šæ­¤æ•°æ®é›†åº”è¯¥é’ˆå¯¹å“ªä¸ªè½´ç»˜åˆ¶(å·¦è¾¹æˆ–è€…å³è¾¹)
      dataSet.setAxisDependency(YAxis.AxisDependency.LEFT);
      // æ˜¯å¦ç»˜åˆ¶æ•°æ®å€¼
      dataSet.setDrawValues(false);


  11.FloatingActionMenu æ§ä»¶ä½¿ç”¨æ–¹å¼
